 public static void main(String[] args) {
       Scanner Nilai = new Scanner (System.in);
    System.out.print("nilai X =");
    Integer X=Nilai.nextInt();
        if(X==1) {
        System.out.println("nilai 1");
        }
        else if(X==2){
    System.out.println("nilai 2");
        }else{
    System.out.println ("nilai salah");
        }
    }
}